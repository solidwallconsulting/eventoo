{% extends app.user.roles[0] =='ROLE_CLIENT'
    ? 'base-client.html.twig' 
    : 'base.html.twig' %}


{% block title %}Eventoo |
	{{ event.name }}
{% endblock %}

{% block body %}


	<!--begin::Content-->
	<div class="content  d-flex flex-column flex-column-fluid" id="kt_content">
		<!--begin::Subheader-->
		<div class="subheader py-2 py-lg-6  subheader-solid " id="kt_subheader">
			<div
				class=" container-fluid  d-flex align-items-center justify-content-between flex-wrap flex-sm-nowrap">
				<!--begin::Info-->
				<div
					class="d-flex align-items-center flex-wrap mr-1">

					<!--begin::Page Heading-->
					<div
						class="d-flex align-items-baseline flex-wrap mr-5">
						<!--begin::Page Title-->
						<h5 class="text-dark font-weight-bold my-1 mr-5">
							{% trans into app.session.get('lng')
                        %}Organisateurs{%
                        endtrans %}
						</h5>
						<!--end::Page Title-->

						<!--begin::Breadcrumb-->
						<ul class="breadcrumb breadcrumb-transparent breadcrumb-dot font-weight-bold p-0 my-2 font-size-sm">
							<li class="breadcrumb-item">
								<a href="/" class="text-muted">{% trans into
                                app.session.get('lng') %}Accueil{% endtrans %}</a>
							</li>
							 
							<li class="breadcrumb-item">
								<a href="" class="text-muted">
									{{event.name}}</a>
							</li>

						</ul>
						<!--end::Breadcrumb-->
					</div>
					<!--end::Page Heading-->
				</div>
				<!--end::Info-->

				<!--begin::Toolbar-->
				<div class="d-flex align-items-center"></div>
				<!--end::Toolbar-->
			</div>
		</div>
		<!--end::Subheader-->

		<!--begin::Entry-->
		<div
			class="d-flex flex-column-fluid">
			<!--begin::Container-->
			<div class=" container ">
				<!--begin::Card-->


				<!-- Modal-->
					<div class="modal fade" id="eventEditModel" tabindex="-1" role="dialog" aria-labelledby="eventEditModelL" aria-hidden="true"> <div class="modal-dialog" role="document">
						<div class="modal-content">
							<div class="modal-header">
								<h5 class="modal-title" id="eventEditModel">{{ event.name }}</h5>
								<button type="button" class="close" data-dismiss="modal" aria-label="Close">
									<i aria-hidden="true" class="ki ki-close"></i>
								</button>
							</div>


							{{ form_start(eventForm) }}
							<div class="modal-body">
								{{ form_widget(eventForm) }}
							</div>


							<div class="modal-footer">
								<button type="button" class="btn btn-light-primary font-weight-bold" data-dismiss="modal">{%
                                trans into app.session.get('lng') %}Fermer{% endtrans %}</button>
								<button type="submit" class="btn btn-primary font-weight-bold">{% trans into
                                app.session.get('lng') %}Modifier{% endtrans %}</button>
							</div>

							{{ form_end(eventForm) }}


						</div>
					</div>
				</div>


				<div class="card card-custom gutter-b">
					<div
						class="card-body">
						<!--begin::Details-->
						<div class="rounded event-cover-image" style="background-image: url({{event.photoURL}})"></div>

						<div
							class="d-flex mb-9">

							<!--begin::Info-->
							<div
								class="flex-grow-1">
								<!--begin::Title-->
								<div class="d-flex justify-content-between flex-wrap mt-1">
									<div class="d-flex mr-3">
										

										

										{% if event.logoURL != null %}
										<img class="rounded mb-4 mr-4" style="width:150px" src="{{event.logoURL}}"  /> 
										{% else %}
											<p><strong>{{event.logoAlt}}</strong></p>
										{% endif %}

										 
										
										
										<a href="#" class="text-dark-75 text-hover-primary font-size-h5 font-weight-bold mr-3  ">
											{{event.name}}
										</a>

										
										

									</div>


									<div class="my-lg-0 my-3">
										<a href="#" class="btn back-btn btn-sm btn-secondary font-weight-bolder text-uppercase mr-3">{% trans into app.session.get('lng') %}Retour{% endtrans %}</a>

										<a href="{{ path('events_show',{id:event.id}) }}" class="btn btn-sm btn-light-info font-weight-bolder text-uppercase mr-3">{% trans into app.session.get('lng') %}Modifier la page d'accueil{% endtrans %}</a>

										<button data-toggle="modal" data-target="#eventEditModel" class="btn btn-sm btn-primary font-weight-bolder text-uppercase mr-3">{% trans into app.session.get('lng') %}Informations de base{% endtrans %}</button>
									</div>
								</div>
								<!--end::Title-->

								<!--begin::Content-->
								<div class="d-flex flex-wrap justify-content-between mt-1">
									<div class="d-flex flex-column flex-grow-1 pr-8">
										<div class="d-flex flex-wrap mb-4">
										

											<a href="#" title="{% trans into app.session.get('lng') %}inscriptions totales{% endtrans %}" class="text-dark-50 text-hover-primary font-weight-bold mr-lg-8 mr-5 mb-lg-0 mb-2">
												<i class="flaticon2-group mr-2 font-size-lg"></i>{{event.totalSubscribersNumber}}</a>
											<a href="#" title="{% trans into app.session.get('lng') %}Date de l'événement{% endtrans %}" class="text-dark-50 text-hover-primary font-weight-bold mr-lg-8 mr-5 mb-lg-0 mb-2">
												<i class="flaticon2-calendar-5 mr-2 font-size-lg"></i>{{ event.startDate | date("m/d/Y g:i:s a") }}
											</a>
											<a href="#" title="{% trans into app.session.get('lng') %}Lieu de l'événement{% endtrans %}" class="text-dark-50 text-hover-primary font-weight-bold mr-lg-8 mr-5 mb-lg-0 mb-2">
												<i class="flaticon2-map mr-2 font-size-lg"></i>
												{{ event.location }}
											</a>


										</div>


									</div>

								</div>
								<!--end::Content-->
							</div>
							<!--end::Info-->
						</div>
						<!--end::Details-->

						<div class="separator separator-solid"></div>

						<!--begin::Items-->
						<div
							class="d-flex align-items-center flex-wrap mt-8">
							<!--begin::Item-->
							<div class="d-flex align-items-center flex-lg-fill mr-5 mb-2">
								<span class="mr-4">
									<i class="flaticon-profile-1 display-4 text-muted font-weight-bold"></i>
								</span>
								<div class="d-flex flex-column text-dark-75">
									<span class="font-weight-bolder font-size-sm">{% trans into app.session.get('lng') %}Profile{% endtrans %}</span>
									<span class="font-weight-bolder font-size-h5">
										<span class="text-dark-50 font-weight-bold"></span>{{event.eventProfiles | length}}</span>
								</div>
							</div>
							<div class="d-flex align-items-center flex-lg-fill mr-5 mb-2">
								<span class="mr-4">
									<i class="flaticon-piggy-bank display-4 text-muted font-weight-bold"></i>
								</span>
								<div class="d-flex flex-column text-dark-75">
									<span class="font-weight-bolder font-size-sm">{% trans into app.session.get('lng') %}Sponsors{% endtrans %}</span>
									<span class="font-weight-bolder font-size-h5">
										<span class="text-dark-50 font-weight-bold"></span>{{event.sponsors | length}}</span>
								</div>
							</div>
							<div class="d-flex align-items-center flex-lg-fill mr-5 mb-2">
								<span class="mr-4">
									<i class="flaticon-businesswoman display-4 text-muted font-weight-bold"></i>
								</span>
								<div class="d-flex flex-column text-dark-75">
									<span class="font-weight-bolder font-size-sm">{% trans into app.session.get('lng') %}Rencontres B2B{% endtrans %}</span>
									<span class="font-weight-bolder font-size-h5">
										<span class="text-dark-50 font-weight-bold"></span>{{event.btBMeetingRooms  | length}}</span>
								</div>
							</div>

							<!--end::Item-->

							 <!--begin::Item-->
							<div class="d-flex align-items-center flex-lg-fill mr-5 mb-2">
								<span class="mr-4">
									<i class="flaticon-confetti display-4 text-muted font-weight-bold"></i>
								</span>
								<div class="d-flex flex-column text-dark-75">
									<span class="font-weight-bolder font-size-sm">{% trans into app.session.get('lng') %}Exposants{% endtrans %}</span>
									<span class="font-weight-bolder font-size-h5">
										<span class="text-dark-50 font-weight-bold"></span>{{event.exposers | length}}</span>
								</div>
							</div>
							<!--end::Item-->

